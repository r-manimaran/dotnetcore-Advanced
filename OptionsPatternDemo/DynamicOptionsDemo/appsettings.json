{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TaxOptions": {
    "TaxRate": 0.06
  },
  "LoggingOptions": {
    "LogLevel": "Information"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }      
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
